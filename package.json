{
  "name": "ethpkg",
  "version": "0.6.0",
  "description": "Sign your packages using crypto currency keys",
  "main": "./dist/index.js",
  "bin": {
    "ethpkg": "./dist/bin/ethpkg.js"
  },
  "scripts": {
    "test": "nyc mocha",
    "static-files": "copyfiles src/signer/eth/metamask/public/index.html -u 1 dist/",
    "copy-build": "yarn static-files && tsc -p ./tsconfig.json",
    "build": "tsc -p ./tsconfig.json",
    "start": "node ./dist/index.js",
    "_publish": "npm pack && ethpkg sign --publish true"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/test/",
      "_test",
      "src/bin",
      "**/*.d.ts"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PhilippLgh/ethereum-signed-packages.git"
  },
  "author": "Philipp Langhans <philipp@ethereum.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PhilippLgh/ethereum-signed-packages/issues"
  },
  "homepage": "https://github.com/PhilippLgh/ethereum-signed-packages#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cli-table": "^0.3.0",
    "@types/es6-promise": "^3.3.0",
    "@types/ethereumjs-util": "^5.2.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/jszip": "^3.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/ora": "^3.0.0",
    "@types/semver": "^6.2.0",
    "@types/tar": "^4.0.3",
    "@types/tar-stream": "^1.6.1",
    "chai": "^4.2.0",
    "copyfiles": "^2.1.0",
    "mocha": "^5.2.0",
    "nock": "^11.7.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@octokit/rest": "^16.35.0",
    "asn1.js": "^5.0.1",
    "base64url": "^3.0.1",
    "boxen": "^4.1.0",
    "chalk": "^2.4.2",
    "cli-table": "^0.3.1",
    "clime": "^0.5.10",
    "enquirer": "^2.3.0",
    "ethereumjs-util": "6.0.0",
    "ethereumjs-wallet": "^0.6.3",
    "file-type": "^10.9.0",
    "form-data": "^3.0.0",
    "jsonwebtoken": "^8.4.0",
    "jszip": "^3.1.5",
    "ora": "^3.1.0",
    "secp256k1": "^3.6.2",
    "semver": "^6.3.0",
    "tar": "^5.0.5",
    "tar-stream": "^2.0.1",
    "xml2js": "^0.4.22"
  }
}
